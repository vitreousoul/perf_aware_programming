{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-O1",
                "${workspaceFolder}/src/repitition_testing.c",
                "-o",
                "${workspaceFolder}/dist/haversine.out",
                "-std=c99",
                "-Wall",
                "-Wextra",
                "-Wstrict-prototypes",
                "-Wold-style-definition",
                "-Wmissing-prototypes",
                "-Wmissing-declarations",
                "-Wno-comment"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
